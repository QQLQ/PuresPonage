#include<iostream>

int main(void)
{
  using namespace std;
  double i,sum=0;
  
  cout<<"please enter the number of money:";
  cin>>i;
  while (i!=0)
   {
    sum+=i;
    cout<<"please enter the number of money:";
    cin>>i;
   }
   cout<<"The sum of your money enter is $"<<sum<<endl;
   return 0;
}
